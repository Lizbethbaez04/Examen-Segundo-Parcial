<UserControl x:Class="ExamenSegundoParcial.ControlInformacionPersonal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock FontSize="15">Nombre:</TextBlock>
        <TextBox Name="txtNombre"></TextBox>
        <TextBlock FontSize="15">Apellido paterno:</TextBlock>
        <TextBox Name="txtApellidoP"></TextBox>
        <TextBlock FontSize="15">Apellido materno:</TextBlock>
        <TextBox Name="txtApellidoM"></TextBox>
        <TextBlock FontSize="15">Edad:</TextBlock>
        <TextBox Name="txtEdad"></TextBox>
    </Grid>
</UserControl>
